{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kubernox": {
    "Domain": "ice-artefact.com",
    "DefaultResolver": "gandiResolver"
  },
  "Proxmox": {
    "Uri": "https://infra.ice-artefact.com",
    "Token": "PVEAPIToken=root@pam!monsupertoken=05449ebc-2eaf-42a6-950a-b6a341f528d0"
  },
  "ConnectionStrings": {
    //"Default": "Data Source=161.97.135.126;Initial Catalog=kubernox;User ID=sa;Password=xxQb6FVes",
    "Default": "Host=5.196.159.55;Database=kubernox;Username=kubernox;Password=kubernoxDbPassword@0123",
    "Redis": "5.196.159.55:6379,password=kubernoxRedisPassword@0123"
  },
  "RabbitMq": {
    "User": "kubernox",
    "Password": "kubernoxRabbitMqPassword0123",
    "HostName": "5.196.159.55",
    "VirtualHost": "/",
    "Port": 5672
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.PostgreSQL.Configuration" ],
    "Enrich": [ "WithClientIp" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Verbose",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "Host=5.196.159.55;Database=kubernox;Username=kubernox;Password=kubernoxDbPassword@0123",
          "tableName": "logs",
          "configurationPath": "Serilog",
          "needAutoCreateTable": true
        }
      }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '%swagger%'"
        }
      }
    ],
    "Columns": {
      "message": "RenderedMessageColumnWriter",
      "message_template": "MessageTemplateColumnWriter",
      "level": {
        "Name": "LevelColumnWriter",
        "Args": {
          "renderAsText": true,
          "dbType": "Varchar"
        }
      },
      "raise_date": "TimestampColumnWriter",
      "exception": "ExceptionColumnWriter",
      "properties": "LogEventSerializedColumnWriter",
      "log_event": "LogEventSerializedColumnWriter",
      "props_test": {
        "Name": "PropertiesColumnWriter",
        "Args": { "dbType": "Json" }
      },
      "machine_name": {
        "Name": "SinglePropertyColumnWriter",
        "Args": {
          "propertyName": "MachineName",
          "writeMethod": "Raw"
        }
      }
    }
  }
}
